<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>flask-manage-events</portlet-name>
		<display-name>Flask Manage Events</display-name>
		<portlet-class>com.rumbasolutions.flask.manageevent.ManageEventPortletAction</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/ManageEvent.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Flask Manage Events</title>
			<short-title>Flask Manage Events</short-title>
			<keywords>Flask Manage Events</keywords>
		</portlet-info>
		
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>flask-admin</role-name>
		</security-role-ref>
		
		<security-role-ref>
			<role-name>flask-content-manager</role-name>
		</security-role-ref>
		
		
	</portlet>
	<portlet>
		<portlet-name>flask-manage-venue</portlet-name>
		<display-name>Flask Manage Venue</display-name>
		<portlet-class>com.rumbasolutions.flask.manageevent.ManageVenuePortletAction</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/ManageVenue.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Flask Manage Venue</title>
			<short-title>Flask Manage Venue</short-title>
			<keywords>Flask Manage Venue</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>flask-admin</role-name>
		</security-role-ref>
		
		<security-role-ref>
			<role-name>flask-content-manager</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>